package Practice;

import java.util.Scanner;

public class ArrayInput {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int leng = scan.nextInt();
		if(leng < 100) {
			int a[] = new int[leng];
			for (int i = 0; i < a.length; i++) {
				a[i] = (int) Math.random() * 100 + 1;
			}
		} else {
			
		}
		
	}

}
