package Practice;

public class Lotto {

	public static void main(String[] args) {
		int[] number = new int[6];
		int i = 0;
		number[i] = (int) (Math.random() * 100 + 1);

		for(i = 1; i < 5; i++) {
			for(int j = 1; )
			if(number[i] != number[i]) {
				number[i] = (int) (Math.random() * 100 + 1);
			}
		}
	}

}
