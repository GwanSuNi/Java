package Practice;

public class Array6 {

	public static void main(String[] args) {
		char[] a = { 'c', 'd' };
		char[] b = { 'c', 'd' };
		boolean judge = false;

		if (a.length == b.length) {
			for (int i = 0; i < a.length; i++) {
				for (int j = 0; j < a.length; j++) {
					if (a[i] == b[i])
						judge = true;
					else
						judge = false;
				}
			}
		} else {

		}

	}

}
