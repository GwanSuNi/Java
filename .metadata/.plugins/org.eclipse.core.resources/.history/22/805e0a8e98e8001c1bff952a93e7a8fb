package Practice;

public class Array6 {

	public static void main(String[] args) {
		char[] a = { 'a', 'd' };
		char[] b = { 'c', 'd' };
		boolean judge = false;

		if (a.length == b.length) {
			for (int i = 0; i < 2; i++) {
				for (int j = 0; j < 2; j++) {
					if (a[i] == b[j])
						judge = true;
					else
						judge = false;
				}
			}
		}
		else {
			
		}
		
		System.out.print(judge);

	}

}
