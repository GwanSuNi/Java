package Practice;

public class Array1 {

	public static void main(String[] args) {

		int a[] = { 1, 2, 3, 4, 5, 6 };
		int sum = 0;

		System.out.println("길이: " + a.length);

		for (var item : a) {
			System.out.print(item + "\t");
		}
		System.out.print("\n3의 배수 : ");

		for (var item : a) {
			if (item % 3 == 0) {
				System.out.print(item + "\t");
			}
		}

	}

}
